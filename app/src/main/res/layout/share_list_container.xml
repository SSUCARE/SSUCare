<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="180dp"
    android:layout_height="180dp"
    android:background="@drawable/layout_border1"
    android:layout_marginBottom="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A님의 건강체크"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/share_list_name"/>
    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="8dp"
        app:layout_constraintTop_toBottomOf="@id/share_list_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.1"
        app:layout_constraintEnd_toStartOf="@id/calendar"
        app:layout_constraintHorizontal_chainStyle="spread"
        android:background="@drawable/layout_border2"
        android:src="@drawable/share_graph"
        android:scaleType="centerInside"
        android:id="@+id/graph"/>
    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="8dp"
        android:background="@drawable/layout_border2"
        app:layout_constraintTop_toTopOf="@id/graph"
        app:layout_constraintStart_toEndOf="@id/graph"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@drawable/share_graph"
        android:scaleType="centerCrop"
        android:id="@+id/calendar"/>
    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="8dp"
        android:background="@drawable/layout_border2"
        app:layout_constraintTop_toBottomOf="@id/graph"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.3"
        app:layout_constraintStart_toStartOf="@id/graph"
        android:src="@drawable/share_graph"
        android:scaleType="centerCrop"
        android:id="@+id/third"/>
    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="8dp"
        android:background="@drawable/layout_border2"
        app:layout_constraintTop_toBottomOf="@id/graph"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.3"
        app:layout_constraintEnd_toEndOf="@id/calendar"
        android:src="@drawable/share_graph"
        android:scaleType="centerCrop"
        android:id="@+id/review"/>
</androidx.constraintlayout.widget.ConstraintLayout>