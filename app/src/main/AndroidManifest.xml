<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.designproject.takecare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.designproject.takecare.KakaoLogin.GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TakeCare"
        android:usesCleartextTraffic="true">

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_native_key" />

        <activity android:name="com.designproject.takecare.UI.MainActivity" />
        <activity android:name="com.designproject.takecare.UI.InfoActivity" />
        <activity android:name="com.designproject.takecare.UI.LoginActivity" />
        <activity android:name="com.designproject.takecare.UI.FindActivity" />
        <activity android:name="com.designproject.takecare.UI.RegisterActivity" />
        <activity
            android:name="com.designproject.takecare.UI.FirstActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>